def quadratic(a,b,c):
    x1= (-b + ((b**2) - 4*a*c)**0.5)/(2*a)
    x2= (-b - ((b**2) - 4*a*c)**0.5)/(2*a)
    print("x is equal to both ", x1 ,"and" , x2)

a = float(input("Enter a value for A: "))
b = float(input("Enter a value for B: "))
c = float(input("Enter a value for C: "))

quadratic(a,b,c)
